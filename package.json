{
  "name": "realty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --scheme=\"realty\" --configuration=\"Debug\" --simulator=\"iPhone 8 (14.5)\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean-cache": "/bin/sh ./bin/clean.sh",
    "clean-android": "cd android && ./gradlew cleanBuildCache",
    "reset": "yarn clean-cache && yarn install && cd ios && rm -rf ./Pods && pod install",
    "apk": "cd android && ./gradlew assembleRelease",
    "bundle": "cd android && ./gradlew bundleRelease",
    "check-types": "tsc --noEmit --project ./tsconfig.json"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^5.3.3",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@types/i18n-js": "^3.8.2",
    "@types/lodash": "^4.14.173",
    "@types/react-native-vector-icons": "^6.4.8",
    "@types/yup": "^0.29.13",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "eslint-plugin-import": "^2.24.2",
    "events": "^3.3.0",
    "formik": "^2.2.0",
    "i18n-js": "^3.7.1",
    "lodash": "^4.17.20",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-react-lite": "^3.2.1",
    "mobx-state-tree": "^5.0.3",
    "normalizr": "^3.6.1",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-localize": "^1.4.2",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^4.4.1",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-sensitive-info": "^6.0.0-alpha.6",
    "react-native-svg": "^11.0.1",
    "react-native-vector-icons": "^7.1.0",
    "typescript": "^4.4.2",
    "yup": "^0.29.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.19",
    "@types/react-native": "^0.64.2",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "husky": "^4.3.0",
